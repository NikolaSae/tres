{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Financial Application Hub - API Documentation",
  "entryPoints": [
    "./actions",
    "./lib",
    "./hooks",
    "./components",
    "./schemas"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "README.md",
  "includeVersion": true,
  "sort": ["source-order"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Actions",
    "Components",
    "Hooks",
    "Utils",
    "Types",
    "*"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "githubPages": false,
  "hideGenerator": false,
  "searchInComments": true,
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": false
  }
}